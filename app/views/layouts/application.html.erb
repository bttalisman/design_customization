<!DOCTYPE html>
<html>
<head>
  <title>Customization</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>


  <%= javascript_include_tag 'bootstrap-colorpicker.js', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'bootstrap.js', 'data-turbolinks-track' => true %>


  <%= stylesheet_link_tag    'bootstrap-colorpicker.css', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'bootstrap.css', media: 'all', 'data-turbolinks-track' => true %>




</head>
<body>

<% flash.each do |key, value| %>
<p><%= value %></p>
<% end %>

<nav>
  <ul>
    <li><%= link_to "Versions", versions_path %></li>
    <li><%= link_to "Templates", design_templates_path %></li>
  </ul>

</nav>





<%= yield %>

</body>
</html>
